<div class="container-fluid">
    <div class="row">
        <div id="left-side" class="col-sm">
            <ul class="list-group">
                {{#each dungeons}}
                <li class="list-group-item">
                    <a href="/dungeon/{{_id}}/battles">
                        Dungeon: {{_id}} {{dungeonName}}
                        {{#each itemsFound}}
                        <img class="dungeonDropImg" src="{{dropImg}}" title="{{dropName}}" />
                        {{/each}}
                    </a>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</div>