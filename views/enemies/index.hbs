<div class="container-fluid enemies index">
  {{#each enemies}}
  <div class="card">
    <div class="card-block">
      <h4 class="card-title"><a href="/dungeon/{{battle.denaDungeonId}}/battles">{{name}}</a></h4>
      <img class="img-fluid" src="{{imgUrl}}" alt="{{name}}" onerror="this.onerror=null;this.src='https://placeholdit.imgix.net/~text?txtsize=100&txt=%3F&w=500&h=190&txttrack=0';" />
    </div>
  </div>
  {{/each}}
</div>